{
  "schemaVersion": 1,
  "id": "shearable_cows",
  "version": "${version}",
  "name": "Shearable Cows",
  "description": "A lightweight mod that lets players shear cows for leather.",
  "authors": [
    "Elijah Potter"
  ],
  "contact": {
    "homepage": "https://github.com/Celestron1123/Shearable-Cows"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/shearable_cows/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "me.elijah.shearable_cows.client.Shearable_cowsDataGenerator"
    ],
    "client": [
      "me.elijah.shearable_cows.client.Shearable_cowsClient"
    ],
    "main": [
      "me.elijah.shearable_cows.Shearable_cows"
    ]
  },
  "mixins": [
    "shearable_cows.mixins.json",
    {
      "config": "shearable_cows.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
